# Coverage configuration
[run]
source = .
omit =
    tests/*
    __pycache__/*
    .venv/*
    venv/*
    */site-packages/*
    conftest.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod

precision = 2
skip_covered = False
skip_empty = True

[html]
directory = htmlcov

[xml]
output = coverage.xml
